@using CarRent.Areas.Admin.ViewModels.About
@model AboutViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers


<form method="POST" asp-route="admin-about-add" enctype="multipart/form-data">
    <div class="form-group">
        <label for="exampleFormControlInput1">Small Header:</label>
        <input class="form-control" id="exampleFormControlInput1" asp-for="@Model.SmallHeader">

        <label for="exampleFormControlInput1">Main Header:</label>
        <input class="form-control" id="exampleFormControlInput1" asp-for="@Model.Header">

        <label for="exampleFormControlInput1">Title:</label>
        <input class="form-control" id="exampleFormControlInput1" asp-for="@Model.Tittle">
        <label for="exampleFormControlInput1">Image:</label>
        <input class="form-control" id="exampleFormControlInput1" asp-for="Image">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>